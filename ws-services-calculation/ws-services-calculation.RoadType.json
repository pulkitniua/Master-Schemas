{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": { "type": "string" },
    "code": { "type": "string" },
    "unitCost": { "type": "number" },
    "isActive": { "type": "boolean" },
    "id": { "type": ["integer", "null"] }
  },
  "additionalProperties": false,
  "required": ["name", "code", "unitCost", "isActive"],
  "x-unique": ["code"]
}
